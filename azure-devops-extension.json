{
    "manifestVersion": 1,
    "id": "gherkin-renderer",
    "publisher": "DogaBendasan",
    "version": "0.0.10",
    "name": "Gherkin Renderer",
    "public":true,
    "description": "Visual Studio Services extension to preview gherkin files in browser",
    "categories": [
        "Azure Repos"
    ],
    "galleryFlags": [
        "Private"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "default_icon_128.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}
